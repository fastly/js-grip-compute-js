{
  "name": "@fastly/grip-compute-js",
  "version": "0.2.0-alpha.0",
  "author": "Fastly, Inc. <oss@fastly.com>",
  "description": "Extension of js-grip for Fastly Compute",
  "contributors": [
    {
      "name": "Katsuyuki Omuro",
      "email": "komuro@fastly.com"
    }
  ],
  "main": "./build/src/index.js",
  "module": "./build/esm/index.js",
  "files": [
    "build/**/*"
  ],
  "types": "./build/src/index.d.ts",
  "scripts": {
    "prepack": "npm run build",
    "build": "npm run lint && npm run build-package",
    "build-package": "npm run build-package:clean && npm run build-package:compile",
    "build-package:clean": "rimraf build",
    "build-package:compile": "tsc --build tsconfig.json && tsc --build tsconfig.esm.json",
    "lint": "npm run lint:eslint",
    "lint:eslint": "eslint src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fastly/js-grip-compute-js.git"
  },
  "readmeFilename": "README.md",
  "keywords": [
    "grip",
    "fanout",
    "fanoutpub",
    "realtime",
    "push",
    "pubcontrol",
    "publish"
  ],
  "license": "MIT",
  "devDependencies": {
    "@fastly/js-compute": "^0.2.4",
    "@types/debug": "^4.1.5",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "eslint": "^8.16.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.7.2"
  },
  "dependencies": {
    "@fanoutio/grip": "^3.1.0"
  }
}
